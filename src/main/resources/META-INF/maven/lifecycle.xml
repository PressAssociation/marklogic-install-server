<lifecycles>
    <lifecycle>
        <id>mlcycle-install</id>
        <phases>
            <phase>
                <id>install</id>
                <executions>
                    <execution>
                        <goals>
                            <goal>bootstrap</goal>
                            <goal>install-databases</goal>
                            <goal>install-servers</goal>
                            <goal>install-cpf</goal>
                            <goal>install-content</goal>
                            <goal>restart-server</goal>
                        </goals>
                    </execution>
                </executions>
            </phase>
        </phases>
    </lifecycle>
    <lifecycle>
        <id>mlcycle-uninstall</id>
        <phases>
            <phase>
                <id>install</id>
                <executions>
                    <execution>
                        <goals>
                            <goal>uninstall-servers</goal>
                            <goal>uninstall-databases</goal>
                        </goals>
                    </execution>
                </executions>
            </phase>
        </phases>
    </lifecycle>
</lifecycles>